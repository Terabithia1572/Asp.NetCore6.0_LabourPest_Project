
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en-US" dir="ltr" data-navigation-type="default" data-navbar-horizontal-shape="default">



<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- ===============================================-->
    <!--    Document Title-->
    <!-- ===============================================-->
    <title>LabourPest Admin Paneli</title>

    <!-- ===============================================-->
    <!--    Favicons-->
    <!-- ===============================================-->
    <link rel="apple-touch-icon" sizes="180x180" href="~/labourpestcustomer/assets/img/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/labourpestcustomer/assets/img/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/labourpestcustomer/assets/img/favicons/favicon-16x16.png">
    <link rel="shortcut icon" type="image/x-icon" href="~/labourpestcustomer/assets/img/favicons/favicon.ico">
    <link rel="manifest" href="~/labourpestcustomer/assets/img/favicons/manifest.json">
    <meta name="msapplication-TileImage" content="~/labourpestcustomer/assets/img/favicons/mstile-150x150.png">
    <meta name="theme-color" content="#ffffff">
    <script src="~/labourpestcustomer/vendors/simplebar/simplebar.min.js"></script>
    <script src="~/labourpestcustomer/assets/js/config.js"></script>

    <!-- ===============================================-->
    <!--    Stylesheets-->
    <!-- ===============================================-->
    <link rel="preconnect" href="https://fonts.googleapis.com/">
    <link rel="preconnect" href="https://fonts.gstatic.com/" crossorigin="">
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:wght@300;400;600;700;800;900&amp;display=swap" rel="stylesheet">
    <link href="~/labourpestcustomer/vendors/simplebar/simplebar.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/labourpestcustomer/unicons.iconscout.com/release/v4.0.8/css/line.css">
    <link href="~/labourpestcustomer/assets/css/theme-rtl.min.css" type="text/css" rel="stylesheet" id="style-rtl">
    <link href="~/labourpestcustomer/assets/css/theme.min.css" type="text/css" rel="stylesheet" id="style-default">
    <link href="~/labourpestcustomer/assets/css/user-rtl.min.css" type="text/css" rel="stylesheet" id="user-style-rtl">
    <link href="~/labourpestcustomer/assets/css/user.min.css" type="text/css" rel="stylesheet" id="user-style-default">
    <script>
        var phoenixIsRTL = window.config.config.phoenixIsRTL;
        if (phoenixIsRTL) {
          var linkDefault = document.getElementById('style-default');
          var userLinkDefault = document.getElementById('user-style-default');
          linkDefault.setAttribute('disabled', true);
          userLinkDefault.setAttribute('disabled', true);
          document.querySelector('html').setAttribute('dir', 'rtl');
        } else {
          var linkRTL = document.getElementById('style-rtl');
          var userLinkRTL = document.getElementById('user-style-rtl');
          linkRTL.setAttribute('disabled', true);
          userLinkRTL.setAttribute('disabled', true);
        }
    </script>
    <link href="~/labourpestcustomer/vendors/glightbox/glightbox.min.css" rel="stylesheet">
</head>

<body>
    <!-- ===============================================-->
    <!--    Main Content-->
    <!-- ===============================================-->
    <main class="main" id="top">

        @await Component.InvokeAsync("_AdminNavbarVerticalViewComponentPartial") <!--Burası Bizim Sol Tarafta Bulunan Kodlarımız Ana Sayfa örneğin-->
        @await Component.InvokeAsync("_AdminTopLogoViewComponentPartial") <!--Burası Yukardıdaki Logonun Bulunduğu alan -->
        @await Component.InvokeAsync("_AdminScriptBottomViewComponentPartial") <!--Burası Logonun Altında Çalışan Script Kodlarının Bulunduğu alan-->
        <div class="content">
            @RenderBody()
            <footer class="footer position-absolute">
                <div class="row g-0 justify-content-between align-items-center h-100">
                    <div class="col-12 col-sm-auto text-center">
                        <p class="mb-0 mt-2 mt-sm-0 text-body">Thank you for creating with Phoenix<span class="d-none d-sm-inline-block"></span><span class="d-none d-sm-inline-block mx-1">|</span><br class="d-sm-none" />2024 &copy;<a class="mx-1" href="https://themewagon.com/">Themewagon</a></p>
                    </div>
                    <div class="col-12 col-sm-auto text-center">
                        <p class="mb-0 text-body-tertiary text-opacity-85">v1.20.1</p>
                    </div>
                </div>
            </footer>
        </div>

    </main><!-- ===============================================-->
    <!--    End of Main Content-->
    <!-- ===============================================-->
    <script src="~/labourpestcustomer/vendors/popper/popper.min.js"></script>
    <script src="~/labourpestcustomer/vendors/bootstrap/bootstrap.min.js"></script>
    <script src="~/labourpestcustomer/vendors/anchorjs/anchor.min.js"></script>
    <script src="~/labourpestcustomer/vendors/is/is.min.js"></script>
    <script src="~/labourpestcustomer/vendors/fontawesome/all.min.js"></script>
    <script src="~/labourpestcustomer/vendors/lodash/lodash.min.js"></script>
    <script src="~/labourpestcustomer/vendors/list.js/list.min.js"></script>
    <script src="~/labourpestcustomer/vendors/feather-icons/feather.min.js"></script>
    <script src="~/labourpestcustomer/vendors/dayjs/dayjs.min.js"></script>
    <script src="~/labourpestcustomer/assets/js/phoenix.js"></script>
    <script src="~/labourpestcustomer/vendors/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/labourpestcustomer/vendors/isotope-packery/packery-mode.pkgd.min.js"></script>
    <script src="~/labourpestcustomer/vendors/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="~/labourpestcustomer/vendors/glightbox/glightbox.min.js"></script>
</body>



</html>
